{
    "compilerOptions": {
        "strict": true,
        "target": "ES6",
        "module": "ESNext",
        "outDir": "dist",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "allowJs": true,
        "checkJs": true,
        "paths": {
          "@/*": ["./src/*"],
        }
    },
    "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
      "exclude": [
        "node_modues"
      ]
}

